{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {
        "eofline": false,
        "quotemark": [
          true,
          "single"
        ]
    },
    "rules": {
      "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case","allow-leading-underscore"]
    },
    "rulesDirectory": []
}